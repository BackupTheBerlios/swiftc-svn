#include "mat_TYPE.h"

Vec3 Mat3x3::operator * (const Vec3& v) 
{ 
    Vec3 result;

    result.x = e00 * v.x + e01 * v.y + e02 * v.z;
    result.y = e10 * v.x + e11 * v.y + e12 * v.z;
    result.z = e20 * v.x + e21 * v.y + e22 * v.z;

    return result;
}
